prototype(Raw.Site:Document.404) < prototype(Raw.Site:Document.AbstractPage) {
    body.content = Neos.Fusion:Component {
        main = Neos.Neos:ContentCollection {
            nodePath = 'main'
        }

        renderer = afx`
            <main id="errorpage" class="main">
                {props.main}
            </main>
        `
    }
}
